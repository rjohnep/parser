{
  "manifest_version": 2,

  "name": "Universal Cart",
  "version": "0.0.2",

  "description": "Universal Cart",
  "icons": {
    "16": "source/16x16.png",
    "48": "source/48x48.png",
    "128": "source/128x128.png"
  },
  "browser_action": {
    "default_title": "Add to wishlist",
    "default_icon": "source/48x48.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "activeTab",
    "http://dev3.madebyewave.com/",
    "http://192.168.58.190/"
  ]
}