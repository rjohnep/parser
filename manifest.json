{
  "manifest_version": 2,
  "name": "Product parser",
  "version": "1.0",
  "icons": {
    "16": "source/16x16.png"
  },
  "browser_action": {
    "default_title": "Add to wishlist",
    "default_icon": "source/48x48.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "background": {
    "scripts": ["js/jquery-2.2.1.min.js", "source/background.js"]
  },
  "permissions": [
    "activeTab",
    "http://dev3.madebyewave.com/"
  ]
}